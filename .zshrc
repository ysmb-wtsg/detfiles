setopt auto_cd

export PATH=/opt/homebrew/bin:$PATH
source /opt/homebrew/share/zsh-autosuggestions/zsh-autosuggestions.zsh

alias d='docker'
alias dc='docker-compose'
alias g='git'
alias lg='lazygit'
alias nv='nvim'
alias py='python'
alias sshkg='ssh-keygen'
alias nvzr='nv ~/.zshrc'
alias nvgc='nv ~/.gitconfig'
alias .z='. ~/.zshrc'
